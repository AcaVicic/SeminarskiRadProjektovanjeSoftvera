<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
	<connectionStrings>
		<add name="RentAConsoleDatabase" connectionString="Data Source=(localdb)\MSSQLLocalDB;Initial Catalog=RentAConsole;Integrated Security=True;Connect Timeout=30;Encrypt=False;TrustServerCertificate=False;ApplicationIntent=ReadWrite;MultiSubnetFailover=False"/>
	</connectionStrings>
	<appSettings>
		<add key="port" value="9999"/>
		<add key="ip" value="127.0.0.1"/>
	</appSettings>
</configuration>